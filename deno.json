{
  "tasks": {
    "dev": "deno run --watch main.ts",
    "make": "deno task db:generate && deno task db:init && deno task db:seed && deno task files:upload",
    "db:generate": "deno run -A npm:drizzle-kit generate",
    "db:init": "deno run -A src/dbified-index-page/init.ts",
    "db:seed": "deno run -A src/dbified-index-page/main.ts",
    "db:studio": "npx drizzle-kit studio",
    "db:clear": "deno run -A src/dbified-index-page/delete-all.ts",
    "files:upload": "deno run -A src/openai-tbc-pages/scripts/upload.ts"
  },
  "imports": {
    "@electric-sql/pglite": "npm:@electric-sql/pglite@^0.3.7",
    "@std/dotenv": "jsr:@std/dotenv@^0.225.5",
    "drizzle-kit": "npm:drizzle-kit@^0.31.4",
    "drizzle-orm": "npm:drizzle-orm@^0.44.4",
    "openai": "npm:openai@^5.12.2",
    "parse5": "npm:parse5@^8.0.0"
  },
  "nodeModulesDir": "auto"
}
